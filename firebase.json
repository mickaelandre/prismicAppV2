{
  "hosting": {
    "public": "prismicapp/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/*",
        "destination": "/index.html"
      },

      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
